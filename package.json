{
  "name": "Sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "test": "npm run mocha-test test/integration",
    "mocha-test": "mocha --recursive --compilers js:babel/register",
    "compile:test": "TARGET=test node tasks/compile.js",
    "compile:staging": "TARGET=staging node tasks/compile.js",
    "install:staging": "PHONE=true TARGET=staging node tasks/compile.js"
  },
  "babel": {
    "whitelist": [
      "es6.modules",
      "es6.classes",
      "es6.arrowFunctions",
      "es6.constants",
      "es6.spread",
      "es6.blockScoping",
      "es6.parameters",
      "es6.destructuring"
    ]
  },
  "dependencies": {
    "events": "^1.0.2",
    "flux": "^2.1.1",
    "invariant": "^2.1.0",
    "keymirror": "^0.1.1",
    "object-assign": "^4.0.1",
    "react-native": "^0.12.0",
    "react-native-keyboardevents": "^0.4.5",
    "react-native-keychain": "^0.2.5",
    "react-native-parsed-text": "0.0.4",
    "react-native-refreshable-listview": "^1.3.0",
    "react-timer-mixin": "^0.13.3",
    "superagent": "^1.4.0"
  },
  "devDependencies": {
    "appium": "1.4.13",
    "babel": "^5.6.0",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "colors": "^1.0.3",
    "ios-deploy": "^1.7.0",
    "koa": "^0.21.0",
    "koa-bodyparser": "^2.0.1",
    "mocha": "^2.2.5",
    "promise": "^7.0.4",
    "underscore": "^1.8.3",
    "wd": "^0.3",
    "yiewd": "^0.6.0"
  }
}
